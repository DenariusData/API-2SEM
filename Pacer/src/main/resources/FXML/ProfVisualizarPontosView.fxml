         <Label fx:id="lblGrupo" prefHeight="27.0" prefWidth="279.0" style="-fx-font-size: 18; -fx-font-weight: bold; -fx-text-fill: #004aad;" text="Grupo: " textAlignment="CENTER">
            <VBox.margin>
               <Insets bottom="10.0" left="15.0" />
            </VBox.margin>
         </Label>
         <Label fx:id="lblSprint" prefHeight="27.0" prefWidth="428.0" style="-fx-font-size: 18; -fx-font-weight: bold; -fx-text-fill: #004aad;" text="Sprint:" textAlignment="CENTER">
            <VBox.margin>
               <Insets bottom="10.0" left="15.0" />
            </VBox.margin>
         </Label>
         <Label fx:id="lblPontosDefinidos" prefHeight="27.0" prefWidth="428.0" style="-fx-font-size: 18; -fx-font-weight: bold; -fx-text-fill: #004aad;" text="Pontos Definidos: " textAlignment="CENTER">
