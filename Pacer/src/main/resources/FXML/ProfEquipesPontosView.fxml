<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="208.0" prefWidth="308.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pacer.professor.ProfEquipesPontosController">
    <children>
        
        <VBox layoutX="4.0" layoutY="6.0" prefHeight="227.0" prefWidth="294.0">
            <HBox spacing="10" />
            <HBox spacing="10" />
           <Label style="-fx-font-size: 18; -fx-font-weight: bold; -fx-text-fill: #004aad;" text="Definir pontos" textAlignment="CENTER">
            <VBox.margin>
               <Insets bottom="10.0" left="15.0" />
            </VBox.margin>
         </Label>
         <Label fx:id="lblGrupo" prefHeight="27.0" prefWidth="279.0" style="-fx-font-size: 18; -fx-font-weight: bold; -fx-text-fill: #004aad;" text="Grupo: " textAlignment="CENTER">
            <VBox.margin>
               <Insets bottom="10.0" left="15.0" />
            </VBox.margin>
         </Label>
         <Label fx:id="lblSprint" prefHeight="27.0" prefWidth="428.0" style="-fx-font-size: 18; -fx-font-weight: bold; -fx-text-fill: #004aad;" text="Sprint:" textAlignment="CENTER">
            <VBox.margin>
               <Insets left="15.0" />
            </VBox.margin>
         </Label>
            <HBox spacing="10" />
            <HBox spacing="10">
                <Label text="Pontos">
               <HBox.margin>
                  <Insets left="15.0" top="20.0" />
               </HBox.margin></Label>
            <TextField fx:id="txtPontos" prefWidth="200.0">
               <HBox.margin>
                  <Insets bottom="20.0" top="20.0" />
               </HBox.margin>
            </TextField>
            <VBox.margin>
               <Insets bottom="15.0" />
            </VBox.margin>
            </HBox>
            <HBox spacing="10" />
            <HBox spacing="10">
            <children>
                  <Button fx:id="btnEnviar" onAction="#enviarPontos" style="-fx-background-color: #004aad; -fx-text-fill: white;" text="Enviar">
                  <HBox.margin>
                     <Insets left="15.0" />
                  </HBox.margin>
               </Button>
                  <Button onAction="#cancelar" style="-fx-background-color: grey; -fx-text-fill: white;" text="Cancelar">
                  <HBox.margin>
                     <Insets bottom="20.0" />
                  </HBox.margin></Button>
            </children></HBox>
            
        </VBox>

        <HBox layoutX="14.0" layoutY="210.0" prefHeight="27.0" prefWidth="0.0" spacing="10" />
    </children>
</AnchorPane>
